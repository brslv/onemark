{
  "name": "onemark",
  "publisher": "brslvg",
  "repository": {
    "type": "git",
    "url": "https://github.com/brslv/onemark.git"
  },
  "displayName": "onemark",
  "description": "Collect lines in your code and quickly navigate to them.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.setMark",
    "onCommand:extension.removeMark",
    "onCommand:extension.clearMarks",
    "onCommand:extension.listMarks"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.setMark",
        "title": "[onemark]: Set Mark"
      },
      {
        "command": "extension.removeMark",
        "title": "[onemark]: Remove Mark"
      },
      {
        "command": "extension.listMarks",
        "title": "[onemark]: List Marks"
      },
      {
        "command": "extension.clearMarks",
        "title": "[onemark]: Clear All Marks"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.42.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  }
}
